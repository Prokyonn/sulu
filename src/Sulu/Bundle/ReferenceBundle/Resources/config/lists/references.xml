<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>references</key>

    <properties>
        <property
            name="referenceTitle"
            translation="sulu_admin.title"
            visibility="yes"
            searchability="yes"
            sortable="true"
            width="shrink"
        >
            <field-name>referenceTitle</field-name>
            <entity-name>%sulu.model.reference.class%</entity-name>
        </property>

        <property
            name="referenceResourceKey"
            translation="sulu_admin.resource"
            visibility="yes"
            searchability="yes"
            sortable="false"
            width="auto"
        >
            <field-name>referenceResourceKey</field-name>
            <entity-name>%sulu.model.reference.class%</entity-name>
        </property>

        <property
            name="referenceResourceId"
            translation="sulu_admin.id"
            visibility="yes"
            searchability="yes"
            sortable="false"
            width="auto"
        />

        <property
            name="referenceProperty"
            translation="sulu_reference.reference_property"
            visibility="yes"
            searchability="no"
            sortable="false"
            width="auto"
        >
            <field-name>referenceProperty</field-name>
            <entity-name>%sulu.model.reference.class%</entity-name>
        </property>
    </properties>
</list>
